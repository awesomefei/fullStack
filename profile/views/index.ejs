<script>

//show slides
var indexOfShow = -1;
var projectsAutomationFlag = true;
runProjectSlides();

function hideSlides(slides){
    for (i = 0; i < slides.length; i++) {
        slides[i].style.display = "none";
    }
}

function nextProjectSlide(positive){
    var slides = document.getElementsByClassName("mySlides");
    hideSlides(slides);

    if(positive){
        console.log("++");
        indexOfShow++;
        if (indexOfShow >= slides.length) {indexOfShow = 0}
        console.log(indexOfShow);
    } else {
        console.log("--");
        indexOfShow--;
        if(indexOfShow < 0) {indexOfShow = slides.length - 1}
        console.log(indexOfShow);
    }

    slides[indexOfShow].style.display = "block";
}

function runProjectSlides() {
    if(projectsAutomationFlag){
        var slides = document.getElementsByClassName("mySlides");
        indexOfShow++;
        if (indexOfShow >= slides.length) {indexOfShow = 0}

        hideSlides(slides);
        slides[indexOfShow].style.display = "block";
    }
    setTimeout(runProjectSlides, 3000); // Change image every 4 seconds
}

$("#projectSlides").hover(function() {
    projectsAutomationFlag = false;
}, function() {
    projectsAutomationFlag = true;
});

</script>
